<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <solid android:color="@color/iaa_bg_dark_gray"/>
            <corners android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:right="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="@color/iaa_white"/>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp"/>
        </shape>
    </item>
</layer-list>
